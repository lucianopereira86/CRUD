<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Registros<h2>
    <table border="1">
        <thead>
            <tr>
                <td>Nome</td>
                <td>Sobrenome</td>
                <td>Ação</td>
            </tr>
        </thead>
        <tbody>
        <% usuario.forEach(function(usuario) { %>
            <tr>
                <td><%= usuario.name %></td>
                <td><%= usuario.surname %></td>
                <td><a href="/edit/<%= usuario._id %>">Editar</a> - <a href="/delete/<%= usuario._id %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
        <button><a href="/">Novo</a></button>
</body>
</html>